{
  "name": "python-like",
  "version": "1.0.1",
  "description": "Source to source compiler using ANTLR",
  "main": "index.js",
  "bin": {
    "pyl": "index.js"
  },
  "scripts": {
    "start": "node index.js",
    "antlr:build": "npm run antlr:clean && npm run antlr:compile",
    "antlr:compile": "npm run help:antlr4-js",
    "antlr:clean": "rm -rf target",
    "help:antlr4-js": "java -Xmx500M -cp '/usr/local/lib/antlr-4.7.1-complete.jar:$CLASSPATH' org.antlr.v4.Tool  -Dlanguage=JavaScript -lib grammars -o target -visitor -no-listener -Xexact-output-dir grammars/Python3.g4",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "author": "Albert Duz",
  "license": "MIT",
  "dependencies": {
    "antlr4": "^4.7.1",
    "js-beautify": "^1.10.0"
  },
  "devDependencies": {
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0"
  }
}
